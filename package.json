{
    "name": "sdet-test",
    "version": "1.0.1",
    "description": "API Automation Test",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test-api": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/ --timeout 10000",
        "reports": "open 'reports/mochawesome.html'",
        "report-temp": "node ./helpers/reporting_temp.js",
        "report-sdetaja": "node ./helpers/reporting_sdetaja.js"
    },
    "author": "SDET Linkaja",
    "license": "ISC",
    "dependencies": {
        "chai": "^4.2.0",
        "chai-json-schema": "^1.5.1",
        "dotenv": "^8.2.0",
        "mocha": "^9.2.2",
        "mochawesome": "^7.1.2",
        "moment-timezone": "^0.5.33",
        "ms-teams-webhook": "^1.0.4",
        "mysql": "^2.18.1",
        "replace-string": "^3.1.0",
        "supertest": "^6.1.1"
    },
    "devDependencies": {
        "eslint": "^8.11.0",
        "eslint-config-standard": "^14.0.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-mocha": "^6.2.2",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.0",
        "husky": "^3.1.0",
        "lint-staged": "^12.3.7",
        "prettier": "^2.2.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "./node_modules/.bin/eslint"
        ]
    },
    "eslintIgnore": [
        "node_modules"
    ]
}
