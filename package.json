{
  "name": "kiwi",
  "version": "1.0.0",
  "description": "API Automation Test",
  "scripts": {
    "test-api": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/ --timeout 30000",
    "reports": "open 'reports/mochawesome.html'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damarmustikoaji/kiwi.git"
  },
  "keywords": [
    "kiwi"
  ],
  "author": "damar mustiko aji",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/damarmustikoaji/kiwi/issues"
  },
  "homepage": "https://github.com/damarmustikoaji/kiwi#readme",
  "dependencies": {
    "chai": "^4.3.6",
    "chai-json-schema": "^1.5.1",
    "dotenv": "^16.0.2",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "supertest": "^6.2.4"
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint"
    ]
  },
  "eslintIgnore": [
    "node_modules"
  ]
}
